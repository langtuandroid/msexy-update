<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"> 
    <item android:id="@android:id/background"> 
        <shape> 
            <corners android:radius="0px" /> 
            <solid android:color="#FFFFFF" />
            <stroke 
                	android:width="1px" 
                	android:color="#000000" 
           	/> 
        </shape> 
    </item> 
    <item android:id="@android:id/secondaryProgress"> 
        <clip> 
            <shape> 
                <corners android:radius="0dip" /> 
                <gradient 
                        android:startColor="#80A4A9A7" 
                        android:centerColor="#807D7D7E" 
                        android:centerY="0.75" 
                        android:endColor="#a0ffcb" 
                        android:angle="270" 
                />
                <stroke
                	android:width="1px" 
                	android:color="#000000" 
           		/> 
            </shape> 
        </clip> 
    </item> 
    <item android:id="@android:id/progress"> 
        <clip> 
            <shape> 
                <corners android:radius="0dip" /> 
                <gradient 
                        android:startColor="#80FCE204" 
                        android:centerColor="#80E2CA03" 
                        android:centerY="0.75" 
                        android:endColor="#499dea" 
                        android:angle="270" 
                />
                <stroke
                	android:width="1px" 
                	android:color="#000000" 
           		/> 
            </shape> 
        </clip> 
    </item> 
</layer-list> 
